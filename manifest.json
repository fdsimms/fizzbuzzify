{
  "manifest_version": 2,

  "name": "Fizzbuzzify",
  "description": "",
  "version": "0.0.0.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["main.js"],
        "run_at": "document_end"
      }
    ],  "permissions": [
    "activeTab",
    "http://*/*",
    "https://ajax.googleapis.com/"
  ]
}
